base='0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz'

def v2r(n, b):
    digits = ''
    while n > 0:
        digits = base[n % b] + digits
        n  = n // b
    print(digits)
    return digits

def r2v(digits, b):
    n = 0
    for d in digits:
        n = b * n + base[:b].index(d)
    print(n)
    return n

def b2b(digits, b1, b2):
    return v2r(r2v(digits, b1), b2)


print(b2b('418',10,15))
